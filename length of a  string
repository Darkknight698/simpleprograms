#include <stdio.h>
int main()
{
    char s[1000];
    int i;
    printf("Enter a string: ");
    //Our string
    scanf("%s", s)
    //With the help of this loop we are able to count the number of characters in the string
    //untill we reach the end
    for(i = 0; s[i] != '\0'; ++i)
    printf("Length of string: %d", i);
    
}
