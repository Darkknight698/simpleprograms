//code in c++;
#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    int frequency = 0;
    cout<<"enter the string :"<<endl;
    cin>>s;
    char c;
    cout<<"enter the char "<<endl;
    cin>>c;
    for(int i=0;i<s.length();i++){
        if(s[i]==c){
            frequency++;
        }
    }
    cout<<c<<" frequency  in the "<<s<<" is "<< frequency<<endl;
    
}
