#include<stdio.h>
int main()
{
   char str[1000], ch;
   int i, frequency = 0;
   print("Enter a string: ");
   gets(str);
   printf("Enter a character to find the frequency: ");
   scanf("%c",&ch);
   for(i = 0; str[i] != '\0'; ++i)
   {
       if(ch == str[i])
           ++frequency;
   }
   printf("Frequency of %c = %d", ch, frequency);
   printf("Frequency of characters in a string is the number of times they occur in a string");
   return 0;
}
